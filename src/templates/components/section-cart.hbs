{{#each items}}    
    <tr>
        <th scope="row">1</th>
        <td>{{name}}</td>
        <td>{{size}}</td>
        <td>{{count}}</td>
        <td>{{price}}</td>
        <td>{{lastprice}}</td>
        <td><button class="btn btn-outline-danger btn-sm" data-size={{size}} data-id={{id}}>Удалить</button></td>
    </tr>
{{/each}}
